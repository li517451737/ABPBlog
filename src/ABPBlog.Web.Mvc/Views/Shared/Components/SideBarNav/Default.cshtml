@using ABPBlog.Web.Views.Shared.Components.SideBarNav
@model SideBarNavViewModel
@{

}
<ul class="sidebar-menu" data-widget="tree">
    @for (var i = 0; i < Model.MainMenu.Items.Count; i++)
    {
        var menuItem = Model.MainMenu.Items[i];
        @Html.Partial("Components/SideBarNav/_UserMenuItem", new UserMenuItemViewModel
       {
           MenuItem = menuItem,
           MenuItemIndex = i,
           RootLevel = true,
           CurrentPageName = Model.ActiveMenuItemName
       })
    }
</ul>


	
